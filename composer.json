{
    "name": "tenqz/qdrant",
    "description": "A powerful PHP library for seamless integration with the Qdrant vector database. Designed to provide an easy-to-use interface for storing, managing, and querying vector data in your PHP applications.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tenqz\\Qdrant\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Oleg Patsay",
            "email": "smmartbiz@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "^7.2|^8.0",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "phpstan/phpstan": "^1.0"
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "cs-fixer-check": "php-cs-fixer fix --dry-run --diff",
        "cs-fixer-fix": "php-cs-fixer fix",
        "check-style": [
            "@cs-check",
            "@cs-fixer-check"
        ],
        "analyze": "phpstan analyse src --level=5",
        "phpunit": "phpunit",
        "test": [
            "@cs-check",
            "@cs-fixer-check",
            "@analyze",
            "@phpunit"
        ]
    }
}
